{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "ES6",
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",

    "typeRoots": ["./node_modules/@types","./node_modules/@typescript"],
  },
  "include": [
    "src/**/*.ts" ],
  "exclude": [
    "node_modules"
  ]
}